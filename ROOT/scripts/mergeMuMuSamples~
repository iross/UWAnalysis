#!/bin/sh

rm EWK.root
hadd EWK.root TOP.root VV.root ZMM.root

rm W.root
hadd W.root WMN.root WTN.root

rm NO_W.root
hadd NO_W.root EWK.root QCD.root ZTT.root

rm NO_Z.root
hadd NO_Z.root W.root TOP.root VV.root QCD.root ZTT.root

rm NO_TOP.root
hadd NO_TOP.root W.root  VV.root QCD.root ZTT.root ZMM.root




