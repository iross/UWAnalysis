#!/bin/sh


preselection='HLT_Any&&pt1>20&&pt2>20&&vertices>0&&diLeptons==0&&lPfRelIsoDeltaBeta<0.1&tauLooseIsoDBPt>0&&abs(dz)<0.2&&lTrigger>0&&tauTrigger>0&&WWMVA>0&&tauElectronMVAPass>0'
oswselection='charge==0&&pZeta<-40&&mt1>70'
ossignalselection='charge==0&pZeta>-20'
sswselection='charge!=0&&pZeta<-50&&mt1>70'
sssignalselection='charge!=0&&pZeta>-20'
qcdselection='pt1>20&&HLT_Any&&vertices>0&&diLeptons==0&&lPfRelIsoDeltaBeta<0.3&&tauLooseIsoDBPt>0.&&charge!=0&&lTrigger>0&&tauTrigger>0&&pZeta>-20&&tauElectronMVAPass>0'
relaxedSelection='pt1>20&&HLT_Any&&vertices>0&&lPfRelIsoDeltaBeta<0.5&charge==0&&pZeta>-20'
vbfSelection0='(nJetsPt30==0||(nJetsPt30==1&&highestJetPt<150.))'
vbfSelection1='(nJetsPt30==1&&highestJetPt>150)'
vbfSelection2='(nJetsPt30>=2&&vbfDEta>4.0&&vbfMass>400&&vbfNJetsGap30==0)'
luminosity=4648.

mkdir eleTauPlots

MakeDataCard channel='eleTau'    dataFile='../inputs/etau/DATA.root' zEmbeddedSample='../inputs/etau/ZTT-EM.root'  wThreeJetsFile='../inputs/etau/W3JETS.root'  zttFile='../inputs/etau/ZJETS.root' zllFile='../inputs/etau/ZJETS.root' wFile='../inputs//etau/W.root' vvFile='../inputs/etau/VV.root' topFile='../inputs/etau/TOP.root' preselection=$preselection       oswselection=$oswselection	       ossignalselection=$ossignalselection         sswselection=$sswselection	      sssignalselection=$sssignalselection  qcdselection=$qcdselection  luminosityErr=0.045 qcdfactor=1.11 qcdfactorErr=0.02 vvErr=0.3  zttScale=1.004 zttScaleErr=0.033    wFactorErr=0.06    zjfterr=0.1  zlfterr=0.24   topErr=0.075 	muID=0.0 muIDErr=0.0 eleID=1.00 eleIDErr=0.01 tauID=1.0 tauIDErr=0.06  dir='../inputs/etau/'  bID=0.94  bIDErr=0.10  bMisID=1.21  bMisIDErr=0.17    bitmask=1,1,1,0  vbfSelection0=$vbfSelection0 vbfSelection1=$vbfSelection1  vbfSelection2=$vbfSelection2   relaxedSelection=$relaxedSelection  weight='__WEIGHT__*__CORR__' vbfFactorZ=1.13 vbfFactorZErr=0.04  vbfFactorW=1.13 vbfFactorWErr=0.04 boostFactorZ=0.95 boostFactorZErr=0.05 boostFactorW=0.95 boostFactorWErr=0.05 luminosity=$luminosity  zlftFactor=0.8 variable='mass' bins=40 min=0.0 max=200 outputFile='eleTauPlots/eleTau_MVis.root'

MakeDataCard channel='eleTau'    dataFile='../inputs/etau/DATA.root' zEmbeddedSample='../inputs/etau/ZTT-EM.root'  wThreeJetsFile='../inputs/etau/W3JETS.root'  zttFile='../inputs/etau/ZJETS.root' zllFile='../inputs/etau/ZJETS.root' wFile='../inputs//etau/W.root' vvFile='../inputs/etau/VV.root' topFile='../inputs/etau/TOP.root' preselection=$preselection       oswselection=$oswselection	       ossignalselection=$ossignalselection         sswselection=$sswselection	      sssignalselection=$sssignalselection  qcdselection=$qcdselection  luminosityErr=0.045 qcdfactor=1.11 qcdfactorErr=0.02 vvErr=0.3  zttScale=1.004 zttScaleErr=0.033    wFactorErr=0.06    zjfterr=0.1  zlfterr=0.24   topErr=0.075 	muID=0.0 muIDErr=0.0 eleID=1.00 eleIDErr=0.01 tauID=1.0 tauIDErr=0.06  dir='../inputs/etau/'  bID=0.94  bIDErr=0.10  bMisID=1.21  bMisIDErr=0.17    bitmask=1,1,1,0  vbfSelection0=$vbfSelection0 vbfSelection1=$vbfSelection1  vbfSelection2=$vbfSelection2   relaxedSelection=$relaxedSelection  weight='__WEIGHT__*__CORR__' vbfFactorZ=1.13 vbfFactorZErr=0.04  vbfFactorW=1.13 vbfFactorWErr=0.04 boostFactorZ=0.95 boostFactorZErr=0.05 boostFactorW=0.95 boostFactorWErr=0.05 luminosity=$luminosity  zlftFactor=0.8 variable='sv_Mass' bins=40 min=0.0 max=300 outputFile='eleTauPlots/eleTau_SVFit.root'

MakeDataCard channel='eleTau'    dataFile='../inputs/etau/DATA.root' zEmbeddedSample='../inputs/etau/ZTT-EM.root'  wThreeJetsFile='../inputs/etau/W3JETS.root'  zttFile='../inputs/etau/ZJETS.root' zllFile='../inputs/etau/ZJETS.root' wFile='../inputs//etau/W.root' vvFile='../inputs/etau/VV.root' topFile='../inputs/etau/TOP.root' preselection=$preselection       oswselection=$oswselection	       ossignalselection=$ossignalselection         sswselection=$sswselection	      sssignalselection=$sssignalselection  qcdselection=$qcdselection  luminosityErr=0.045 qcdfactor=1.11 qcdfactorErr=0.02 vvErr=0.3  zttScale=1.004 zttScaleErr=0.033    wFactorErr=0.06    zjfterr=0.1  zlfterr=0.24   topErr=0.075 	muID=0.0 muIDErr=0.0 eleID=1.00 eleIDErr=0.01 tauID=1.0 tauIDErr=0.06  dir='../inputs/etau/'  bID=0.94  bIDErr=0.10  bMisID=1.21  bMisIDErr=0.17    bitmask=1,1,1,0  vbfSelection0=$vbfSelection0 vbfSelection1=$vbfSelection1  vbfSelection2=$vbfSelection2   relaxedSelection=$relaxedSelection  weight='__WEIGHT__*__CORR__' vbfFactorZ=1.13 vbfFactorZErr=0.04  vbfFactorW=1.13 vbfFactorWErr=0.04 boostFactorZ=0.95 boostFactorZErr=0.05 boostFactorW=0.95 boostFactorWErr=0.05 luminosity=$luminosity  zlftFactor=0.8 variable='mass' bins=30 min=0.0 max=600 outputFile='eleTauPlots/eleTau_MVisLS.root'

